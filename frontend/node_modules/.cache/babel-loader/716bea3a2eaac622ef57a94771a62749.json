AAHA,iBAAA,CAAA,CAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,sBAAA,OAAA,GAAA;AACA,0BAAA,QAAA,CAAA,IAAA,CAAA,EADA;AAEA,mCAAA,MAAA,CAAA,WAFA;AAGA,8BAAA,MAAA,CAAA;AAHA,mBAAA;AAKA,kBAAA,OAAA,CAAA,GAAA,CAAA,OAAA;;AACA,kBAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA;;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,WAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,MAAA,GAAA,CAAA;AACA,iBAlBA,EAkBA,KAlBA,CAkBA,UAAA,KAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,iBApBA,CAFA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwBA,KA/BA;AAgCA,IAAA,SAhCA,qBAgCA,OAhCA,EAgCA,MAhCA,EAgCA;AACA,UAAA,IAAA,GAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,OAAA;AAAA,OAAA,EAAA,CAAA,CAAA;AACA,UAAA,WAAA,GAAA,IAAA,CAAA,WAAA;AACA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,GAAA,EAAA,wCAAA,OAAA,GAAA,GAFA;AAGA,QAAA,OAAA,EAAA;AACA,0BAAA;AADA,SAHA;AAMA,QAAA,IAAA,EAAA;AACA,UAAA,MAAA,EAAA,MADA;AAEA,UAAA,WAAA,EAAA;AAFA;AANA,OAAA,CAAA,CAUA,IAVA,CAUA,YAAA;AACA,QAAA,IAAA,CAAA,MAAA,GAAA,MAAA;AACA,OAZA;AAaA,KAhDA;AAiDA,IAAA,UAjDA,sBAiDA,OAjDA,EAiDA;AAAA;;AACA,UAAA,IAAA,GAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,OAAA;AAAA,OAAA,EAAA,CAAA,CAAA;AACA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,QADA;AAEA,QAAA,GAAA,EAAA,wCAAA,OAAA,GAAA,GAFA;AAGA,QAAA,OAAA,EAAA;AACA,0BAAA;AADA;AAHA,OAAA,CAAA,CAMA,IANA,CAMA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA;AACA,OARA;AASA;AA5DA;AAZA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n    <h1 class=\"title\">vue todo</h1>\n\n    <hr>\n\n    <div id=\"main_form\" class=\"card\">\n        <form v-on:submit.prevent=\"addTask\">\n        <div id=\"inside_stuff\">\n        <h2 class=\"subtitle\">Add task</h2>\n           <div class=\"field\">\n              <label class=\"label\">description</label>\n              <div class=\"control\">\n                <input id=\"desc_input\" class=\"input\" type=\"text\" v-model=\"description\">\n              </div>\n           </div>\n\n           <div class=\"field\">\n             <label class=\"label\">status</label>\n             <div class=\"control\">\n                <div class=\"select\">\n                   <select v-model=\"status\">\n                    <option value=\"1\">to do</option>\n                    <option value=\"2\">done</option>\n                   </select>\n                </div>\n             </div>\n           </div>\n\n           <div class=\"field\">\n             <div class=\"control\">\n               <button class=\"btn btn-success\">submit</button>\n             </div>\n           </div>\n           </div>\n        </form>\n      </div>\n\n      <div class=\"columns\">\n        <div class=\"column is-6\">\n          <h2 class=\"subtitle\">to do</h2>\n\n          <div class=\"todo\">\n            <div class=\"card\" v-for=\"task in tasks\" v-if=\"task.status === 1\" v-bind:key=\"task.id\">\n              <div class=\"card-content\">\n              <div class=\"left_content\">\n                {{ task.description }}\n              </div>\n                <button class=\"right_btn btn btn-danger\" @click=\"deleteTask(task.id)\">delete</button>\n                <button class=\"right_btn btn btn-outline-warning\" @click=\"setStatus(task.id, 2)\">done</button>\n              </div>\n              </div>\n            </div>\n          </div>\n          <div class=\"column is-6\">\n             <h2 class=\"subtitle\">done</h2>\n            <div class=\"todo\">\n            <div class=\"card\" v-for=\"task in tasks\" v-if=\"task.status === 2\" v-bind:key=\"task.id\">\n              <div class=\"card-content\">\n              <div class=\"left_content\">\n                {{ task.description }}\n              </div>\n                <button class=\"right_btn btn btn-danger\" @click=\"deleteTask(task.id)\">delete</button>\n                <button class=\"right_btn btn btn-primary\">edit</button>\n                <button class=\"right_btn btn btn-outline-warning\" @click=\"setStatus(task.id, 1)\">to do</button>\n              </div>\n              </div>\n            </div>\n          </div>\n          </div>\n        </div>\n  </div>\n</template>\n\n<script>\n\nimport axios from 'axios'\n\nexport default {\n  name: 'Home',\n  data () {\n    return {\n        tasks: [],\n        description: '',\n        status: 'todo'\n    }\n  },\n  mounted () {\n    this.getTasks()\n  },\n  methods: {\n    getTasks () {\n       axios({\n          method: 'get',\n          url: 'http://127.0.0.1:8000/source/tasks/'\n        }).then(response => this.tasks = response.data)\n    },\n    async addTask() {\n      if (this.description) {\n        await axios({\n          method: 'post',\n          url: 'http://127.0.0.1:8000/source/tasks/',\n          data: {\n            description: this.description,\n            status: this.status\n          },\n        }).then((response) => {\n          let newTask = {\n            'id': response.data.id,\n            'description': this.description,\n            'status': this.status\n          }\n          console.log(newTask)\n          this.tasks.push(newTask)\n          this.getTasks()\n          this.description = ''\n          this.status = 2\n        }).catch((error) => {\n          console.log(error)\n        })\n      }\n    },\n    setStatus(task_id, status) {\n      const task = this.tasks.filter(task => task.id === task_id)[0]\n      const description = task.description\n      axios({\n        method: 'put',\n        url: 'http://127.0.0.1:8000/source/tasks/' + task_id + '/',\n        headers: {\n          'Content-Type': 'application/json',\n        },\n        data: {\n          status: status,\n          description: description\n        },\n      }).then(() => {\n        task.status = status\n      })\n    },\n    deleteTask(task_id) {\n      const task = this.tasks.filter(task => task.id === task_id)[0]\n      axios({\n        method: 'delete',\n        url: 'http://127.0.0.1:8000/source/tasks/' + task_id + '/',\n        headers: {\n          'Content-Type': 'application/json',\n        },\n      }).then(() => {\n        this.getTasks()\n      })\n    }\n  }\n}\n\n</script>\n<style lang=\"scss\">\n.select, select {\n  width: 40%;\n}\n\n.card {\n  margin-bottom: 20px;\n}\n\n.done {\n  opacity: 0.3;\n}\n\n.right_btn {\n    float: right;\n    margin-right: 6px;\n}\n\n.card_content {\n    text-align: center;\n}\n\n.left_content {\n float: left;\n}\n\n.columns {\n    margin-left: 0.25rem;\n    margin-right: 0.25rem;\n}\n\n#main_form {\n    width: 50%;\n    margin:auto;\n    margin-bottom: 60px;\n}\n\n#desc_input {\n    width: 75%;\n}\n\n#inside_stuff {\n    margin-bottom: 20px;\n    margin-top: 18px;\n}\n</style>"],"sourceRoot":"src/views"}]}